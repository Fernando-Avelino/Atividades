#include <stdio.h>
#include <stdlib.h>
int main()
{
int vetor[10];
int i;
float soma = 0;

for (i = 0; i < 10; i++) 
{
  printf("Digite o valor da posicao %d do vetor: ", i+1);
  scanf("%d", &vetor[i]);
  soma = soma + vetor[i];
  }
float media = soma/10;
printf("\nO valor da media aritmetica dos elementos do vetor e: %.2f\n",media);

return 0;
}
